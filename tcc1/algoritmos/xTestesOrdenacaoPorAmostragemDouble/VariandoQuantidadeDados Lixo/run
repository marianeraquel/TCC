Variando a quantidade de dados (4 máquinas)

bin/start-all.sh 

bin/hadoop dfs -rmr /user/OrdenacaoPorAmostragem/QuantidadeDados/

bin/hadoop jar GeraDados.jar <path> <potencia> <cenarios> <arquivos>

10⁶	bin/hadoop jar exe/GeraDados.jar /user/raquel/OrdenacaoPorAmostragem/QuantidadeDados 6 1 1
10⁷	bin/hadoop jar exe/GeraDados.jar /user/raquel/OrdenacaoPorAmostragem/QuantidadeDados 7 1 10
10⁸	bin/hadoop jar exe/GeraDados.jar /user/raquel/OrdenacaoPorAmostragem/QuantidadeDados 8 1 100
10⁹	bin/hadoop jar exe/GeraDados.jar /user/raquel/OrdenacaoPorAmostragem/QuantidadeDados 9 1 1000
10¹⁰	bin/hadoop jar exe/GeraDados.jar /user/raquel/OrdenacaoPorAmostragem/QuantidadeDados 10 1 10000


bin/hadoop dfs -text /user/raquel/OrdenacaoPorAmostragem/QuantidadeDados/Uniforme6_Folder1-File1 | wc

bin/hadoop jar OrdenacaoPorAmostragem.jar <dir-dfs-entrada><particoes><freq_min><freq_max><freq_passo><amostras_min><amostras_max><anostras_passo><execucoes><dir-resultados><arq-resultados>

bin/hadoop jar exe/OrdenacaoPorAmostragem.jar /user/raquel/OrdenacaoPorAmostragem/ 8 0.9 0.9 0.1 10000 10000 1 3 /home/raquel/houtput Uniforme6_1out1.txt Uniforme6_1out2.txt

bin/hadoop jar exe/OrdenacaoPorAmostragem.jar /user/raquel/OrdenacaoPorAmostragem/QuantidadeDados/ 8 0.9 0.9 0.1 10000 10000 1 3 /home/marianehadoop/houtput Uniforme6_2out1.txt Uniforme6_2out2.txt

bin/hadoop jar exe/OrdenacaoPorAmostragem.jar /user/raquel/OrdenacaoPorAmostragem/QuantidadeDados/ 8 0.9 0.9 0.1 10000 10000 1 3 /home/marianehadoop/houtput Uniforme6_3out1.txt Uniforme6_3out2.txt

bin/hadoop dfs -text /user/raquel/OrdenacaoPorAmostragem/QuantidadeDados/Uniforme6_Folder1-0.9-10000-1/part-00000 | head


bin/stop-all.sh 
